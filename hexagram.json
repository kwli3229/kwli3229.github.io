{
  "易經": {
    "䷀": {
      "卦名": "乾",
      "中文": "乾為天",
      "英文": "The Creative",
      "卦序": 1,
      "卦辭": "元，亨，利，貞。",
      "彖辭": "大哉乾元，萬物資始，乃統天。雲行雨施，品物流形。大明終始，六位時成，時乘六龍以御天。乾道變化，各正性命，保合大和，乃利貞。首出庶物，萬國咸寧。",
      "象辭": "天行健，君子以自強不息。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "潛龍，勿用。", "象辭": "潛龍勿用，陽在下也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "見龍在田，利見大人。", "象辭": "見龍在田，德施普也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "君子終日乾乾，夕惕若，厲，无咎。", "象辭": "終日乾乾，反復道也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "或躍在淵，无咎。", "象辭": "或躍在淵，進無咎也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "飛龍在天，利見大人。", "象辭": "飛龍在天，大人造也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "亢龍，有悔。", "象辭": "亢龍有悔，盈不可久也。" },
        "用": { "陰陽":"用九", "數":9, "爻辭": "見群龍无首，吉。", "象辭": "用九，天德不可為首也。" }
      }
    },
    "䷁": {
      "卦名": "坤",
      "中文": "坤為地",
      "英文": "The Receptive",
      "卦序": 2,
      "卦辭": "元，亨，利牝馬之貞。君子有攸往，先迷後得主，利。西南得朋，東北喪朋。安貞，吉。",
      "彖辭": "至哉坤元，萬物資生，乃順承天。坤厚載物，德合无疆。含弘光大，品物咸亨。牝馬地類，行地无疆，柔順利貞。君子攸行，先迷失道，後順得常。西南得朋，乃與類行﹔東北喪朋，乃終有慶。安貞之吉，應地无疆。",
      "象辭": "地勢坤，君子以厚德載物。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "履霜，堅冰至。", "象辭": "履霜堅冰，陰始凝也。馴致其道，至堅冰也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "直，方，大，不習无不利。", "象辭": "六二之動，直以方也。不習无不利，地道光也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "含章可貞，或從王事，无成有終。", "象辭": "含章可貞，以時發也。或從王事，知光大也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "括囊，无咎无譽。", "象辭": "括囊无咎，慎不害也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "黃裳，元吉。", "象辭": "黃裳元吉，文在中也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "龍戰于野，其血玄黃。", "象辭": "龍戰于野，其道窮也。" },
        "用": { "陰陽":"用六", "數":6, "爻辭": "利永貞。", "象辭": "用六永貞，以大終也。" }
      }
    },
    "䷂": {
      "卦名": "屯",
      "中文": "水雷屯",
      "英文": "Initial Difficulty",
      "卦序": 3,
      "卦辭": "元，亨，利，貞。勿用，有攸往，利建侯。",
      "彖辭": "屯，剛柔始交而難生，動乎險中，大亨貞。雷雨之動滿盈，天造草昧，宜建侯而不寧。",
      "象辭": "雲雷，屯﹔君子以經綸。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "磐桓，利居貞，利建侯。", "象辭": "雖磐桓，志行正也。以貴下賤，大得民也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "屯如邅如，乘馬班如。匪寇婚媾，女子貞不字，十年乃字。", "象辭": "六二之難，乘剛也。十年乃字，反常也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "即鹿无虞，惟入于林中，君子幾不如舍，往吝。", "象辭": "即鹿无虞，以從禽也。君子舍之，往吝窮也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "乘馬班如，求婚媾，往吉，无不利。", "象辭": "求而往，明也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "屯其膏，小貞吉，大貞凶。", "象辭": "屯其膏，施未光也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "乘馬班如，泣血漣如。", "象辭": "泣血漣如，何可長也。" }
      }
    },
    "䷃": {
      "卦名": "蒙",
      "中文": "山水蒙",
      "英文": "Youthful Folly",
      "卦序": 4,
      "卦辭": "亨。匪我求童蒙，童蒙求我。初筮告，再三瀆，瀆則不告。利貞。",
      "彖辭": "蒙，山下有險，險而止，蒙。蒙亨，以亨行時中也。匪我求童蒙，童蒙求我，志應也。初筮告，以剛中也。再三瀆，瀆則不告，瀆蒙也。蒙以養正，聖功也。",
      "象辭": "山下出泉，蒙﹔君子以果行育德。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "發蒙，利用刑人，用說桎梏，以往吝。", "象辭": "利用刑人，以正法也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "包蒙吉。納婦吉。子克家。", "象辭": "子克家，剛柔接也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "勿用取女，見金夫，不有躬，无攸利。", "象辭": "勿用取女，行不順也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "困蒙，吝。", "象辭": "困蒙之吝，獨遠實也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "童蒙，吉。", "象辭": "童蒙之吉，順以巽也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "擊蒙，不利為寇，利禦寇。", "象辭": "利用禦寇，上下順也。" }
      }
    },
    "䷄": {
      "卦名": "需",
      "中文": "水天需",
      "英文": "Waiting",
      "卦序": 5,
      "卦辭": "有孚，光亨，貞吉。利涉大川。",
      "彖辭": "需，須也。險在前也，剛健而不陷，其義不困窮矣。需有孚，光亨，貞吉，位乎天位，以正中也。利涉大川，往有功也。",
      "象辭": "雲上於天，需﹔君子以飲食宴樂。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "需于郊，利用恆，无咎。", "象辭": "需于郊，不犯難行也。利用恆，无咎，未失常也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "需于沙，小有言，終吉。", "象辭": "需于沙，衍在中也。雖小有言，以終吉也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "需于泥，致寇至。", "象辭": "需于泥，災在外也。致寇至，憂災也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "需于血，出自穴。", "象辭": "需于血，順以聽也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "需于酒食，貞吉。", "象辭": "酒食貞吉，以中正也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "入于穴，有不速之客三人來，敬之終吉。", "象辭": "不速之客來，敬之終吉，雖不當位，未大失也。" }
      }
    },
    "䷅": {
      "卦名": "訟",
      "中文": "天水訟",
      "英文": "Conflict",
      "卦序": 6,
      "卦辭": "有孚，窒惕，中吉，終凶。利見大人，不利涉大川。",
      "彖辭": "訟，上剛下險，險而健，訟。訟有孚，窒惕，中吉，剛來而得中也。終凶，訟不可成也。利見大人，尚中正也。不利涉大川，入于淵也。",
      "象辭": "天與水違行，訟﹔君子以做事謀始。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "不永所事，小有言，終吉。", "象辭": "不永所事，訟不可長也。雖小有言，其辭明也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "不克訟，歸而逋，其邑人三百戶，无眚。", "象辭": "不克訟，歸而逋也。自下訟上，患至掇也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "食舊德，貞厲，終吉。或從王事，无成。", "象辭": "食舊德，從上吉也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "不克訟，復即命，渝，安貞，吉。", "象辭": "復即命，渝，安貞不失也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "訟，元吉。", "象辭": "訟，元吉，以中正也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "或錫之鞶帶，終朝三褫之。", "象辭": "以訟受服，亦不足敬也。" }
      }
    },
    "䷆": {
      "卦名": "師",
      "中文": "地水師",
      "英文": "The Army",
      "卦序": 7,
      "卦辭": "貞，丈人，吉，无咎。",
      "彖辭": "師，眾也，貞正也，能以眾正，可以王矣。剛中而應，行險而順，以此毒天下，而民從之，吉又何咎矣。",
      "象辭": "地中有水，師﹔君子以容民畜眾。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "師出以律，否臧凶。", "象辭": "師出以律，失律凶也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "在師中，吉，无咎，王三錫命。", "象辭": "在師中吉，承天寵也。王三錫命，懷萬邦也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "師或輿尸，凶。", "象辭": "師或輿尸，大无功也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "師左次，无咎。", "象辭": "左次无咎，未失常也。" },
        "五": { "陰陽":"九五", "數":6, "爻辭": "田有禽，利執言，无咎。長子帥師，弟子輿尸，貞凶。", "象辭": "長子帥師，以中行也。弟子輿尸，使不當也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "大君有命，開國承家，小人勿用。", "象辭": "大君有命，以正功也。小人勿用，必亂邦也。" }
      }
    },
    "䷇": {
      "卦名": "比",
      "中文": "水地比",
      "英文": "Following",
      "卦序": 8,
      "卦辭": "比，吉。原筮，元永貞，无咎。不寧方來，後夫凶。",
      "彖辭": "比，吉也。比，輔也，下順從也。原筮元永貞，无咎，以剛中也。不寧方來，上下應也。後夫凶，其道窮也。",
      "象辭": "地上有水，比﹔先王以建萬國，親諸侯。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "有孚比之，无咎。有孚盈缶，終來有他，吉。", "象辭": "比之初六，有他吉也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "比之自內，貞吉。", "象辭": "比之自內，不從如也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "比之匪人。", "象辭": "比之匪人，不亦傷乎。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "外比之，貞吉。", "象辭": "外比於賢，以從上也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "顯比，王用三驅，失前禽。邑人不誡，吉。", "象辭": "顯比之吉，位正中也。舍逆取順，失前禽也。邑人不誡，上使中也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "比之无首，凶。", "象辭": "比之无首，无所終也。" }
      }
    },
    "䷈": {
      "卦名": "小畜",
      "中文": "風天小畜",
      "英文": "Small Accumulation",
      "卦序": 9,
      "卦辭": "亨。密雲不雨，自我西郊。",
      "彖辭": "小畜，柔得位而上下應之，曰小畜。健而巽，剛中而志行，乃亨。密雲不雨，尚往也。自我西郊，施未行也。",
      "象辭": "風行天上，小畜﹔君子以懿文德。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "復自道，何其咎，吉。", "象辭": "復自道，其義吉也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "牽復，吉。", "象辭": "牽復在中，亦不自失也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "輿說輻，夫妻反目。", "象辭": "夫妻反目，不能正室也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "有孚，血去惕出，无咎。", "象辭": "有孚惕出，上合志也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "有孚攣如，富以其鄰。", "象辭": "有孚攣如，不獨富也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "既雨既處，尚德載，婦貞厲。月幾望，君子征凶。", "象辭": "既雨既處，德積載也。君子征凶，有所疑也。" }
      }
    },
    "䷉": {
      "卦名": "履",
      "中文": "天澤履",
      "卦序": 10,
      "卦辭": "履虎尾，不咥人，亨。",
      "彖辭": "履，柔履剛也。說而應乎乾，是以履虎尾，不咥人，亨。剛中正，履帝位而不疚，光明也。",
      "象辭": "上天下澤，履﹔君子以辨上下，定民志。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "素履，往无咎。", "象辭": "素履之往，獨行愿也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "履道坦坦，幽人貞吉。", "象辭": "幽人貞吉，中不自亂也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "眇能視，跛能履，履虎尾，咥人，凶。武人為于大君。", "象辭": "眇能視，不足以有明也。跛能履，不足以行也。咥人之凶，位不當也。武人為于大君，志剛也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "履虎尾，愬愬，終吉。", "象辭": "愬愬終吉，志行也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "夬履，貞厲。", "象辭": "夬履貞厲，位正當也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "視履考祥，其旋元吉。", "象辭": "元吉在上，大有慶也。" }
      }
    },
    "䷊": {
      "卦名": "泰",
      "中文": "地天泰",
      "卦序": 11,
      "卦辭": "小往大來，吉亨。",
      "彖辭": "泰，小往大來，吉亨。則是天地交，而萬物通也﹔上下交，而其志同也。內陽而外陰，內健而外順，內君子而外小人，君子道長，小人道消也。",
      "象辭": "天地交泰，后以財成天地之道，輔相天地之宜，以左右民。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "拔茅茹，以其彙，征吉。", "象辭": "拔茅征吉，志在外也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "包荒，用馮河，不遐遺，朋亡，得尚于中行。", "象辭": "包荒得尚于中行，以光大也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "无平不陂，无往不復，艱貞无咎。勿恤其孚，于食有福。", "象辭": "无往不復，天地際也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "翩翩，不富以其鄰，不戒以孚。", "象辭": "翩翩不富，皆失實也。不戒以孚，中心願也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "帝乙歸妹，以祉元吉。", "象辭": "以祉元吉，中以行願也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "城復于隍，勿用師。自邑告命，貞吝。", "象辭": "城復于隍，其命亂也。" }
      }
    },
    "䷋": {
      "卦名": "否",
      "中文": "天地否",
      "卦序": 12,
      "卦辭": "否之匪人，不利君子貞，大往小來。",
      "彖辭": "否之匪人，不利君子貞，大往小來。則是天地不交，而萬物不通也﹔上下不交，而天下无邦也。內陰而外陽，內柔而外剛，內小人而外君子，小人道長，君子道消也。",
      "象辭": "天地不交，否﹔君子以儉德辟難，不可榮以祿。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "拔茅茹，以其彙，貞吉，亨。", "象辭": "拔茅貞吉，志在君也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "包承，小人吉，大人否，亨。", "象辭": "大人否亨，不亂群也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "包羞。", "象辭": "包羞，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "有命无咎，疇離祉。", "象辭": "有命无咎，志行也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "休否，大人吉。其亡其亡，繫于苞桑。", "象辭": "大人之吉，位正當也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "傾否，先否後喜。", "象辭": "否終則傾，何可長也。" }
      }
    },
    "䷌": {
      "卦名": "同人",
      "中文": "天地同人",
      "英文": "Influence",
      "卦序": 13,
      "卦辭": "同人于野，亨。利涉大川，利君子貞。",
      "彖辭": "同人，柔得位得中，而應乎乾，曰同人。同人曰，同人于野，亨。利涉大川，乾行也。文明以健，中正而應，君子正也。唯君子為能通天下之志。",
      "象辭": "天與火，同人﹔君子以類族辨物。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "同人于門，无咎。", "象辭": "出門同人，又誰咎也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "同人于宗，吝。", "象辭": "同人于宗，吝道也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "伏戎于莽，升其高陵，三歲不興。", "象辭": "伏戎于莽，敵剛也。三歲不興，安行也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "乘其墉，弗克攻，吉。", "象辭": "乘其墉，義弗克也。其吉，則困而反則也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "同人，先號咷而後笑。大師克相遇。", "象辭": "同人之先，以中直也。大師相遇，言相克也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "同人于郊，无悔。", "象辭": "同人于郊，志未得也。" }
      }
    },
    "䷍": {
      "卦名": "大有",
      "中文": "火天大有",
      "英文": "Abundance",
      "卦序": 14,
      "卦辭": "元亨。",
      "彖辭": "大有，柔得尊位，大中而上下應之，曰大有。其德剛健而文明，應乎天而時行，是以元亨。",
      "象辭": "火在天上，大有﹔君子以遏惡揚善，順天休命。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "无交害，匪咎，艱則无咎。", "象辭": "大有初九，无交害也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "大車以載，有攸往，无咎。", "象辭": "大車以載，積中不敗也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "公用亨于天子，小人弗克。", "象辭": "公用亨于天子，小人害也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "匪其彭，无咎。", "象辭": "匪其彭，无咎﹔明辨晰也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "厥孚交如，威如，吉。", "象辭": "厥孚交如，信以發志也。威如之吉，易而无備也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "自天祐之，吉无不利。", "象辭": "大有上吉，自天祐也。" }
      }
    },
    "䷎": {
      "卦名": "謙",
      "中文": "地山謙",
      "英文": "Humility",
      "卦序": 15,
      "卦辭": "亨，君子有終。",
      "彖辭": "謙亨，天道下濟而光明，地道卑而上行。天道虧盈而益謙，地道變盈而流謙，鬼神害盈而福謙，人道惡盈而好謙。謙尊而光，卑而不可踰，君子之終也。",
      "象辭": "地中有山，謙﹔君子以裒多益寡，稱物平施。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "謙謙君子，用涉大川，吉。", "象辭": "謙謙君子，卑以自牧也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "鳴謙，貞吉。", "象辭": "鳴謙貞吉，中心得也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "勞謙，君子有終，吉。", "象辭": "勞謙君子，萬民服也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "无不利，撝謙。", "象辭": "无不利，撝謙﹔不違則也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "不富以其鄰，利用侵伐，无不利。", "象辭": "利用侵伐，征不服也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "鳴謙，利用行師，征邑國。", "象辭": "鳴謙志未得，可用行師征邑國也。" }
      }
    },
    "䷏": {
      "卦名": "豫",
      "中文": "雷地豫",
      "英文": "Providing",
      "卦序": 16,
      "卦辭": "利建侯行師。",
      "彖辭": "豫，剛應而志行，順以動，豫。豫，順以動，故天地如之，而況建侯行師乎？天地以順動，故日月不過，而四時不忒。聖人以順動，則刑罰清而民服。豫之時義大矣哉！",
      "象辭": "雷出地奮，豫﹔先王以作樂崇德，殷薦之上帝，以配祖考。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "鳴豫，凶。", "象辭": "初六鳴豫，志窮凶也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "介于石，不終日，貞吉。", "象辭": "不終日，貞吉﹔以中正也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "盱豫，悔。遲有悔。", "象辭": "盱豫有悔，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "由豫，大有得。勿疑，朋盍簪。", "象辭": "由豫大有得，志大行也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "貞疾，恆不死。", "象辭": "六五貞疾，乘剛也。恆不死，中未亡也。" },
        "上": { "陰陽":"上九", "數":6, "爻辭": "冥豫，成有渝，无咎。", "象辭": "冥豫在上，何可長也。" }
      }
    },
    "䷐": {
      "卦名": "隨",
      "中文": "澤水隨",
      "英文": "Following",
      "卦序": 17,
      "卦辭": "元亨利貞，无咎。",
      "彖辭": "隨，剛來而下柔，動而說，隨。大亨貞，无咎，而天下隨時，隨時之義大矣哉！",
      "象辭": "澤中有雷，隨﹔君子以向晦入宴息。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "官有渝，貞吉。出門交有功。", "象辭": "官有渝，從正吉也。出門交有功，不失也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "係小子，失丈夫。", "象辭": "係小子，弗兼與也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "係丈夫，失小子。隨有求得，利居貞。", "象辭": "係丈夫，志舍下也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "隨有獲，貞凶。有孚在道，以明，何咎。", "象辭": "隨有獲，其義凶也。有孚在道，明功也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "孚于嘉，吉。", "象辭": "孚于嘉，吉﹔位正中也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "拘係之，乃從維之。王用亨于西山。", "象辭": "拘係之，上窮也。" }
      }
    },
    "䷑": {
      "卦名": "蠱",
      "中文": "山風蠱",
      "英文": "Contagion",
      "卦序": 18,
      "卦辭": "元亨，利涉大川。先甲三日，後甲三日。",
      "彖辭": "蠱，剛上而柔下，巽而止，蠱。蠱，元亨，而天下治也。利涉大川，往有事也。先甲三日，後甲三日，終則有始，天行也。",
      "象辭": "山下有風，蠱﹔君子以振民育德。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "幹父之蠱，有子，考无咎，厲終吉。", "象辭": "幹父之蠱，意承考也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "幹母之蠱，不可貞。", "象辭": "幹母之蠱，得中道也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "幹父之蠱，小有悔，无大咎。", "象辭": "幹父之蠱，終无咎也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "裕父之蠱，往見吝。", "象辭": "裕父之蠱，往未得也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "幹父之蠱，用譽。", "象辭": "幹父用譽，承以德也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "不事王侯，高尚其事。", "象辭": "不事王侯，志可則也。" }
      }
    },
    "䷒": {
      "卦名": "臨",
      "中文": "澤地臨",
      "英文": "Approach",
      "卦序": 19,
      "卦辭": "元亨利貞。至于八月有凶。",
      "彖辭": "臨，剛浸而長。說而順，剛中而應，大亨以正，天之道也。至于八月有凶，消不久也。",
      "象辭": "澤上有地，臨﹔君子以教思无窮，容保民无疆。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "咸臨，貞吉。", "象辭": "咸臨貞吉，志行正也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "咸臨，吉，无不利。", "象辭": "咸臨，吉无不利﹔未順命也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "甘臨，无攸利。既憂之，无咎。", "象辭": "甘臨，位不當也。既憂之，咎不長也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "至臨，无咎。", "象辭": "至臨无咎，位當也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "知臨，大君之宜，吉。", "象辭": "大君之宜，行中之謂也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "敦臨，吉，无咎。", "象辭": "敦臨之吉，志在內也。" }
      }
    },
    "䷓": {
      "卦名": "觀",
      "中文": "風天觀",
      "英文": "Viewing",
      "卦序": 20,
      "卦辭": "盥而不薦，有孚顒若。",
      "彖辭": "大觀在上，順而巽，中正以觀天下。觀，盥而不薦，有孚顒若，下觀而化也。觀天之神道，而四時不忒，聖人以神道設教，而天下服矣。",
      "象辭": "風行地上，觀﹔先王以省方觀民設教。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "童觀，小人无咎，君子吝。", "象辭": "初六童觀，小人道也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "窺觀，利女貞。", "象辭": "窺觀女貞，亦可丑也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "觀我生，進退。", "象辭": "觀我生，進退﹔未失道也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "觀國之光，利用賓于王。", "象辭": "觀國之光，尚賓也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "觀我生，君子无咎。", "象辭": "觀我生，觀民也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "觀其生，君子无咎。", "象辭": "觀其生，志未平也。" }
      }
    },
    "䷔": {
      "卦名": "噬嗑",
      "中文": "火雷噬嗑",
      "英文": "Biting Through",
      "卦序": 21,
      "卦辭": "亨。利用獄。",
      "彖辭": "頤中有物，曰噬嗑，噬嗑而亨。剛柔分，動而明，雷電合而章。柔得中而上行，雖不當位，利用獄也。",
      "象辭": "雷電噬嗑﹔先王以明罰敕法。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "屨校滅趾，无咎。", "象辭": "屨校滅趾，不行也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "噬膚滅鼻，无咎。", "象辭": "噬膚滅鼻，乘剛也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "噬臘肉，遇毒﹔小吝，无咎。", "象辭": "遇毒，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "噬乾胏，得金矢，利艱貞，吉。", "象辭": "利艱貞吉，未光也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "噬乾肉，得黃金，貞厲，无咎。", "象辭": "貞厲无咎，得當也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "何校滅耳，凶。", "象辭": "何校滅耳，聰不明也。" }
      }
    },
    "䷕": {
      "卦名": "賁",
      "中文": "山火賁",
      "英文": "Grace",
      "卦序": 22,
      "卦辭": "亨。小利有攸往。",
      "彖辭": "賁，亨，柔來而文剛，故亨。分剛上而文柔，故小利有攸往。天文也。文明以止，人文也。觀乎天文，以察時變﹔觀乎人文，以化成天下。",
      "象辭": "山下有火，賁﹔君子以明庶政，无敢折獄。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "賁其趾，舍車而徒。", "象辭": "舍車而徒，義弗乘也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "賁其須。", "象辭": "賁其須，與上興也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "賁如濡如，永貞吉。", "象辭": "永貞之吉，終莫之陵也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "賁如皤如，白馬翰如，匪寇婚媾。", "象辭": "六四，當位疑也。匪寇婚媾，終无尤也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "賁于丘園，束帛戔戔，吝，終吉。", "象辭": "六五之吉，有喜也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "白賁，无咎。", "象辭": "白賁无咎，上得志也。" }
      }
    },
    "䷖": {
      "卦名": "剝",
      "中文": "山地剝",
      "英文": "Splitting Apart",
      "卦序": 23,
      "卦辭": "不利有攸往。",
      "彖辭": "剝，剝也，柔變剛也。不利有攸往，小人長也。順而止之，觀象也。君子尚消息盈虛，天行也。",
      "象辭": "山附于地，剝﹔上以厚下，安宅。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "剝床以足，蔑貞凶。", "象辭": "剝床以足，以滅下也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "剝床以辨，蔑貞凶。", "象辭": "剝床以辨，未有與也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "剝之，无咎。", "象辭": "剝之无咎，失上下也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "剝床以膚，凶。", "象辭": "剝床以膚，切近災也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "貫魚，以宮人寵，无不利。", "象辭": "以宮人寵，終无尤也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "碩果不食，君子得輿，小人剝廬。", "象辭": "君子得輿，民所載也。小人剝廬，終不可用也。" }
      }
    },
    "䷗": {
      "卦名": "復",
      "中文": "地雷復",
      "英文": "Return",
      "卦序": 24,
      "卦辭": "亨。出入无疾，朋來无咎。反復其道，七日來復，利有攸往。",
      "彖辭": "復，亨。剛反，動而以順行，是以出入无疾，朋來无咎。反復其道，七日來復，天行也。利有攸往，剛長也。復，其見天地之心乎。",
      "象辭": "雷在地中，復﹔先王以至日閉關，商旅不行，后不省方。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "不遠復，无祗悔，元吉。", "象辭": "不遠之復，以修身也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "休復，吉。", "象辭": "休復之吉，以下仁也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "頻復，厲，无咎。", "象辭": "頻復之厲，義无咎也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "中行獨復。", "象辭": "中行獨復，以從道也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "敦復，无悔。", "象辭": "敦復无悔，中以自考也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "迷復，凶，有災眚。用行師，終有大敗，以其國君，凶。至于十年不克征。", "象辭": "迷復之凶，反君道也。" }
      }
    },
    "䷘": {
      "卦名": "无妄",
      "中文": "天雷无妄",
      "英文": "Innocence",
      "卦序": 25,
      "卦辭": "元，亨，利，貞。其匪正有眚，不利有攸往。",
      "彖辭": "无妄，剛自外來而為主於內。動而健，剛中而應，大亨以正，天之命也。其匪正有眚，不利有攸往，無妄之往，何之矣。天命不佑，行矣哉。",
      "象辭": "天下雷行，物與无妄﹔先王以茂對時，育萬物。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "无妄，往吉。", "象辭": "无妄之往，得志也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "不耕獲，不菑畬，則利有攸往。", "象辭": "不耕獲，未富也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "无妄之災，或繫之牛，行人之得，邑人之災。", "象辭": "行人得牛，邑人災也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "可貞，无咎。", "象辭": "可貞无咎，固有之也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "无妄之疾，勿藥有喜。", "象辭": "无妄之藥，不可試也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "无妄，行有眚，无攸利。", "象辭": "无妄之行，窮之災也。" }
      }
    },
    "䷙": {
      "卦名": "大畜",
      "中文": "山天大畜",
      "英文": "Great Accumulation",
      "卦序": 26,
      "卦辭": "利貞，不家食吉，利涉大川。",
      "彖辭": "大畜，剛健篤實輝光，日新其德，剛上而尚賢。能止健，大正也。不家食吉，養賢也。利涉大川，應乎天也。",
      "象辭": "天在山中，大畜﹔君子以多識前言往行，以畜其德。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "有厲，利已。", "象辭": "有厲利已，不犯災也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "輿說輹。", "象辭": "輿說輹，中无尤也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "良馬逐，利艱貞。曰閑輿衛，利有攸往。", "象辭": "利有攸往，上合志也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "童牛之牿，元吉。", "象辭": "六四元吉，有喜也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "豶豕之牙，吉。", "象辭": "六五之吉，有慶也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "何天之衢，亨。", "象辭": "何天之衢，道大行也。" }
      }
    },
    "䷚": {
      "卦名": "頤",
      "中文": "山雷頤",
      "英文": "Nourishment",
      "卦序": 27,
      "卦辭": "貞吉。觀頤，自求口實。",
      "彖辭": "頤，貞吉，養正則吉也。觀頤，觀其所養也﹔自求口實，觀其自養也。天地養萬物，聖人養賢，以及萬民﹔頤之時義大矣哉。",
      "象辭": "山下有雷，頤﹔君子以慎言語，節飲食。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "舍爾靈龜，觀我朵頤，凶。", "象辭": "觀我朵頤，亦不足貴也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "顛頤，拂經，于丘頤，征凶。", "象辭": "六二征凶，行失類也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "拂頤，貞凶，十年勿用，无攸利。", "象辭": "十年勿用，道大悖也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "顛頤，吉。虎視眈眈，其欲逐逐，无咎。", "象辭": "顛頤之吉，上施光也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "拂經，居貞吉，不可涉大川。", "象辭": "居貞之吉，順以從上也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "由頤，厲吉，利涉大川。", "象辭": "由頤厲吉，大有慶也。" }
      }
    },
    "䷛": {
      "卦名": "大過",
      "中文": "澤風大過",
      "英文": "Great Exceeding",
      "卦序": 28,
      "卦辭": "棟橈，利有攸往，亨。",
      "彖辭": "大過，大者過也。棟橈，本末弱也。剛過而中，巽而說行。利有攸往，乃亨。大過之時大矣哉。",
      "象辭": "澤滅木，大過﹔君子以獨立不懼，遯世无悶。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "藉用白茅，无咎。", "象辭": "藉用白茅，柔在下也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "枯楊生稊，老夫得其女妻，无不利。", "象辭": "老夫女妻，過以相與也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "棟橈，凶。", "象辭": "棟橈之凶，不可以有輔也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "棟隆，吉。有它，吝。", "象辭": "棟隆之吉，不橈乎下也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "枯楊生華，老婦得其士夫，无咎无譽。", "象辭": "枯楊生華，何可久也。老婦士夫，亦可醜也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "過涉滅頂，凶，无咎。", "象辭": "過涉之凶，不可咎也。" }
      }
    },
    "䷜": {
      "卦名": "坎",
      "中文": "坎為水",
      "英文": "The Abysmal Water",
      "卦序": 29,
      "卦辭": "習坎，有孚，維心亨，行有尚。",
      "彖辭": "習坎，重險也。水流而不盈，行險而不失其信。維心亨，乃以剛中也。行有尚，往有功也。天險不可升也，地險山川丘陵也，王公設險以守其國，坎之時用大矣哉。",
      "象辭": "水洊至，習坎﹔君子以常德行，習教事。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "習坎，入于坎窞，凶。", "象辭": "習坎入坎，失道凶也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "坎有險，求小得。", "象辭": "求小得，未出中也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "來之坎坎，險且枕，入于坎窞，勿用。", "象辭": "來之坎坎，終无功也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "樽酒簋貳，用缶，納約自牖，終无咎。", "象辭": "樽酒簋貳，剛柔際也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "坎不盈，祗既平，无咎。", "象辭": "坎不盈，中未大也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "係用徽纆，寘于叢棘，三歲不得，凶。", "象辭": "上六失道，凶三歲也。" }
      }
    },
    "䷝": {
      "卦名": "離",
      "中文": "離為火",
      "英文": "The Clinging Fire",
      "卦序": 30,
      "卦辭": "利貞，亨。畜牝牛，吉。",
      "彖辭": "離，麗也﹔日月麗乎天，百穀草木麗乎土，重明以麗乎正，乃化成天下。柔麗乎中正，故亨﹔是以畜牝牛吉也。",
      "象辭": "明兩作，離﹔大人以繼明照于四方。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "履錯然，敬之，无咎。", "象辭": "履錯之敬，以辟咎也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "黃離，元吉。", "象辭": "黃離元吉，得中道也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "日昃之離，不鼓缶而歌，則大耋之嗟，凶。", "象辭": "日昃之離，何可久也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "突如，其來如，焚如，死如，棄如。", "象辭": "突如其來如，无所容也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "出涕沱若，戚嗟若，吉。", "象辭": "六五之吉，離王公也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "王用出征，有嘉折首，獲匪其醜，无咎。", "象辭": "王用出征，以正邦也。" }
      }
    },
    "䷞": {
      "卦名": "咸",
      "中文": "澤山咸",
      "英文": "Influence",
      "卦序": 31,
      "卦辭": "亨，利貞，取女吉。",
      "彖辭": "咸，感也。柔上而剛下，二氣感應以相與，止而說，男下女，是以亨利貞，取女吉也。天地感而萬物化生，聖人感人心而天下和平﹔觀其所感，而天地萬物之情可見矣。",
      "象辭": "山上有澤，咸﹔君子以虛受人。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "咸其拇。", "象辭": "咸其拇，志在外也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "咸其腓，凶，居吉。", "象辭": "雖凶居吉，順不害也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "咸其股，執其隨，往吝。", "象辭": "咸其股，亦不處也。志在隨人，所執下也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "貞吉，悔亡，憧憧往來，朋從爾思。", "象辭": "貞吉悔亡，未感害也。憧憧往來，未光大也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "咸其脢，无悔。", "象辭": "咸其脢，志末也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "咸其輔，頰，舌。", "象辭": "咸其輔頰舌，滕口說也。" }
      }
    },
    "䷟": {
      "卦名": "恆",
      "中文": "雷風恆",
      "英文": "Duration",
      "卦序": 32,
      "卦辭": "亨，无咎，利貞，利有攸往。",
      "彖辭": "恆，久也。剛上而柔下，雷風相與，巽而動，剛柔皆應，恆。恆亨无咎，利貞﹔久於其道也，天地之道，恆久而不已也。利有攸往，終則有始也。日月得天，而能久照，四時變化，而能久成，聖人久於其道，而天下化成﹔觀其所恆，而天地萬物之情可見矣。",
      "象辭": "雷風，恆﹔君子以立不易方。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "浚恆，貞凶，无攸利。", "象辭": "浚恆之凶，始求深也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "悔亡。", "象辭": "九二悔亡，能久中也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "不恆其德，或承之羞，貞吝。", "象辭": "不恆其德，無所容也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "田无禽。", "象辭": "久非其位，安得禽也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "恆其德，貞，婦人吉，夫子凶。", "象辭": "婦人貞吉，從一而終也。夫子制義，從婦凶也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "振恆，凶。", "象辭": "振恆在上，大无功也。" }
      }
    },
    "䷠": {
      "卦名": "遯",
      "中文": "天山遯",
      "英文": "Retreat",
      "卦序": 33,
      "卦辭": "亨，小利貞。",
      "彖辭": "遯亨，遯而亨也，剛當位而應，與時行也。小利貞，浸而長也。遯之時義大矣哉。",
      "象辭": "天下有山，遯﹔君子以遠小人，不惡而嚴。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "遯尾，厲，勿用有攸往。", "象辭": "遯尾之厲，不往何災也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "執之用黃牛之革，莫之勝說。", "象辭": "執用黃牛，固志也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "係遯，有疾厲，畜臣妾吉。", "象辭": "係遯之厲，有疾憊也。畜臣妾吉，不可大事也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "好遯，君子吉，小人否。", "象辭": "君子好遯，小人否也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "嘉遯，貞吉。", "象辭": "嘉遯貞吉，以正志也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "肥遯，无不利。", "象辭": "肥遯无不利，无所疑也。" }
      }
    },
    "䷡": {
      "卦名": "大壯",
      "中文": "雷天大壯",
      "英文": "Great Power",
      "卦序": 34,
      "卦辭": "利貞。",
      "彖辭": "大壯，大者壯也。剛以動，故壯。大壯利貞﹔大者正也。正大而天地之情可見矣。",
      "象辭": "雷在天上，大壯﹔君子以非禮弗履。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "壯于趾，征凶，有孚。", "象辭": "壯于趾，其孚窮也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "貞吉。", "象辭": "九二貞吉，以中也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "小人用壯，君子用罔，貞厲。羝羊觸藩，羸其角。", "象辭": "小人用壯，君子罔也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "貞吉悔亡，藩決不羸，壯于大輿之輹。", "象辭": "藩決不羸，尚往也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "喪羊于易，无悔。", "象辭": "喪羊于易，位不當也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "羝羊觸藩，不能退，不能遂，无攸利，艱則吉。", "象辭": "不能退，不能遂，不祥也。艱則吉，咎不長也。" }
      }
    },
    "䷢": {
      "卦名": "晉",
      "中文": "火地晉",
      "英文": "Progress",
      "卦序": 35,
      "卦辭": "晉。康侯用錫馬蕃庶，晝日三接。",
      "彖辭": "晉，進也。明出地上，順而麗乎大明，柔進而上行。是以康侯用錫馬蕃庶，晝日三接也。",
      "象辭": "明出地上，晉﹔君子以自昭明德。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "晉如，摧如，貞吉。罔孚，裕，无咎。", "象辭": "晉如摧如，獨行正也。裕无咎，未受命也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "晉如，愁如，貞吉。受茲介福，于其王母。", "象辭": "受茲介福，以中正也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "眾允，悔亡。", "象辭": "眾允之，志上行也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "晉如鼫鼠，貞厲。", "象辭": "鼫鼠貞厲，位不當也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "悔亡，失得勿恤，往吉，无不利。", "象辭": "失得勿恤，往有慶也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "晉其角，維用伐邑，厲吉无咎，貞吝。", "象辭": "維用伐邑，道未光也。" }
      }
    },
    "䷣": {
      "卦名": "明夷",
      "中文": "地火明夷",
      "英文": "Darkening of the Light",
      "卦序": 36,
      "卦辭": "利艱貞。",
      "彖辭": "明入地中，明夷。內文明而外柔順，以蒙大難，文王以之。利艱貞，晦其明也，內難而能正其志，箕子以之。",
      "象辭": "明入地中，明夷﹔君子以蒞眾，用晦而明。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "明夷于飛，垂其翼。君子于行，三日不食，有攸往，主人有言。", "象辭": "君子于行，義不食也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "明夷，夷于左股，用拯馬壯，吉。", "象辭": "六二之吉，順以則也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "明夷于南狩，得其大首，不可疾貞。", "象辭": "南狩之志，乃大得也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "入于左腹，獲明夷之心，于出門庭。", "象辭": "入于左腹，獲心意也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "箕子之明夷，利貞。", "象辭": "箕子之貞，明不可息也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "不明晦，初登于天，後入于地。", "象辭": "初登于天，照四國也。後入于地，失則也。" }
      }
    },
    "䷤": {
      "卦名": "家人",
      "中文": "風火家人",
      "英文": "The Family",
      "卦序": 37,
      "卦辭": "利女貞。",
      "彖辭": "家人，女正位乎內，男正位乎外，男女正，天地之大義也。家人有嚴君焉，父母之謂也。父父，子子，兄兄，弟弟，夫夫，婦婦，而家道正﹔正家而天下定矣。",
      "象辭": "風自火出，家人﹔君子以言有物，而行有恆。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "閑有家，悔亡。", "象辭": "閑有家，志未變也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "无攸遂，在中饋，貞吉。", "象辭": "六二之吉，順以巽也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "家人嗃嗃，悔厲吉﹔婦子嘻嘻，終吝。", "象辭": "家人嗃嗃，未失也。婦子嘻嘻，失家節也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "富家，大吉。", "象辭": "富家大吉，順在位也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "王假有家，勿恤，吉。", "象辭": "王假有家，交相愛也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "有孚威如，終吉。", "象辭": "威如之吉，反身之謂也。" }
      }
    },
    "䷥": {
      "卦名": "睽",
      "中文": "火澤睽",
      "英文": "Opposition",
      "卦序": 38,
      "卦辭": "小事吉。",
      "彖辭": "睽，火動而上，澤動而下﹔二女同居，其志不同行。說而麗乎明，柔進而上行，得中而應乎剛﹔是以小事吉。天地睽，而其事同也﹔男女睽，而其志通也﹔萬物睽，而其事類也﹔睽之時用大矣哉。",
      "象辭": "上火下澤，睽﹔君子以同而異。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "悔亡，喪馬勿逐，自復﹔見惡人，无咎。", "象辭": "見惡人，以辟咎也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "遇主于巷，无咎。", "象辭": "遇主于巷，未失道也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "見輿曳，其牛掣，其人天且劓，无初有終。", "象辭": "見輿曳，位不當也。无初有終，遇剛也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "睽孤，遇元夫，交孚，厲无咎。", "象辭": "交孚无咎，志行也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "悔亡，厥宗噬膚，往何咎。", "象辭": "厥宗噬膚，往有慶也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "睽孤，見豕負塗，載鬼一車，先張之弧，後說之弧，匪寇婚媾，往遇雨則吉。", "象辭": "遇雨之吉，群疑亡也。" }
      }
    },
    "䷦": {
      "卦名": "蹇",
      "中文": "水山蹇",
      "英文": "Obstruction",
      "卦序": 39,
      "卦辭": "利西南，不利東北﹔利見大人，貞吉。",
      "彖辭": "蹇，難也，險在前也。見險而能止，知矣哉。蹇利西南，往得中也﹔不利東北，其道窮也。利見大人，往有功也。當位貞吉，以正邦也。蹇之時用大矣哉。",
      "象辭": "山上有水，蹇﹔君子以反身修德。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "往蹇，來譽。", "象辭": "往蹇來譽，宜待也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "王臣蹇蹇，匪躬之故。", "象辭": "王臣蹇蹇，終无尤也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "往蹇來反。", "象辭": "往蹇來反，內喜之也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "往蹇來連。", "象辭": "往蹇來連，當位實也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "大蹇朋來。", "象辭": "大蹇朋來，以中節也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "往蹇來碩，吉﹔利見大人。", "象辭": "往蹇來碩，志在內也。利見大人，以從貴也。" }
      }
    },
    "䷧": {
      "卦名": "解",
      "中文": "雷水解",
      "英文": "Deliverance",
      "卦序": 40,
      "卦辭": "利西南，无所往，其來復吉。有攸往，夙吉。",
      "彖辭": "解，險以動，動而免乎險，解。解利西南，往得眾也﹔其來復吉，乃得中也。有攸往夙吉，往有功也。天地解，而雷雨作，雷雨作，而百果草木皆甲坼，解之時大矣哉。",
      "象辭": "雷雨作，解﹔君子以赦過宥罪。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "无咎。", "象辭": "剛柔之際，義无咎也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "田獲三狐，得黃矢，貞吉。", "象辭": "九二貞吉，得中道也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "負且乘，致寇至，貞吝。", "象辭": "負且乘，亦可醜也。自我致戎，又誰咎也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "解而拇，朋至斯孚。", "象辭": "解而拇，未當位也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "君子維有解，吉﹔有孚于小人。", "象辭": "君子有解，小人退也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "公用射隼，于高墉之上，獲之，无不利。", "象辭": "公用射隼，以解悖也。" }
      }
    },
    "䷨": {
      "卦名": "損",
      "中文": "山澤損",
      "英文": "Decrease",
      "卦序": 41,
      "卦辭": "有孚，元吉，无咎，可貞，利有攸往。曷之用，二簋可用享。",
      "彖辭": "損，損下益上，其道上行。損而有孚，元吉，无咎，可貞，利有攸往。曷之用？二簋可用享。二簋應有時。損剛益柔有時，損益盈虛，與時偕行。",
      "象辭": "山下有澤，損﹔君子以懲忿窒欲。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "已事遄往，无咎，酌損之。", "象辭": "已事遄往，尚合志也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "利貞，征凶，弗損益之。", "象辭": "九二利貞，中以為志也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "三人行，則損一人﹔一人行，則得其友。", "象辭": "一人行，三則疑也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "損其疾，使遄有喜，无咎。", "象辭": "損其疾，亦可喜也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "或益之，十朋之龜弗克違，元吉。", "象辭": "六五元吉，自上佑也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "弗損益之，无咎，貞吉，利有攸往，得臣无家。", "象辭": "弗損益之，大得志也。" }
      }
    },
    "䷩": {
      "卦名": "益",
      "中文": "風雷益",
      "英文": "Increase",
      "卦序": 42,
      "卦辭": "利有攸往，利涉大川。",
      "彖辭": "益，損上益下，民說无疆，自上下下，其道大光。利有攸往，中正有慶。利涉大川，木道乃行。益動而巽，日進无疆。天施地生，其益無方。凡益之道，與時偕行。",
      "象辭": "風雷，益﹔君子以見善則遷，有過則改。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "利用為大作，元吉，无咎。", "象辭": "元吉无咎，下不厚事也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "或益之，十朋之龜弗克違，永貞吉。王用享于帝，吉。", "象辭": "或益之，自外來也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "益之用凶事，无咎。有孚中行，告公用圭。", "象辭": "益用凶事，固有之也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "中行，告公從。利用為依遷國。", "象辭": "告公從，以益志也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "有孚惠心，勿問元吉。有孚惠我德。", "象辭": "有孚惠心，勿問之矣。惠我德，大得志也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "莫益之，或擊之，立心勿恆，凶。", "象辭": "莫益之，偏辭也。或擊之，自外來也。" }
      }
    },
    "䷪": {
      "卦名": "夬",
      "中文": "澤天夬",
      "英文": "Breakthrough",
      "卦序": 43,
      "卦辭": "揚于王庭，孚號，有厲，告自邑，不利即戎，利有攸往。",
      "彖辭": "夬，決也，剛決柔也。健而說，決而和，揚于王庭，柔乘五剛也。孚號有厲，其危乃光也。告自邑，不利即戎，所尚乃窮也。利有攸往，剛長乃終也。",
      "象辭": "澤上於天，夬；君子以施祿及下，居德則忌。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "壯于前趾，往不勝為咎。", "象辭": "不勝而往，咎也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "惕號，莫夜有戎，勿恤。", "象辭": "有戎勿恤，得中道也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "壯于頄，有凶。君子夬夬，獨行，遇雨，若濡，有慍，无咎。", "象辭": "君子夬夬，終无咎也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "臀无膚，其行次且。牽羊悔亡，聞言不信。", "象辭": "其行次且，位不當也。聞言不信，聰不明也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "莧陸夬夬，中行无咎。", "象辭": "中行无咎，中未光也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "无號，終有凶。", "象辭": "无號之凶，終不可長也。" }
      }
    },
    "䷫": {
      "卦名": "姤",
      "中文": "天風姤",
      "英文": "Coming to Meet",
      "卦序": 44,
      "卦辭": "女壯，勿用取女。",
      "彖辭": "姤，遇也，柔遇剛也。勿用取女，不可與長也。天地相遇，品物咸章也。剛遇中正，天下大行也。姤之時義大矣哉。",
      "象辭": "天下有風，姤；后以施命誥四方。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "繫于金柅，貞吉，有攸往，見凶，羸豕踟躅。", "象辭": "繫于金柅，柔道牽也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "包有魚，无咎，不利賓。", "象辭": "包有魚，義不及賓也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "臀无膚，其行次且，厲，无大咎。", "象辭": "其行次且，行未牽也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "包无魚，起凶。", "象辭": "无魚之凶，遠民也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "以杞包瓜，含章，有隕自天。", "象辭": "九五含章，中正也。有隕自天，志不舍命也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "姤其角，吝，无咎。", "象辭": "姤其角，上窮吝也。" }
      }
    },
    "䷬": {
      "卦名": "萃",
      "中文": "澤地萃",
      "英文": "Gathering Together",
      "卦序": 45,
      "卦辭": "亨。王假有廟，利見大人，亨，利貞。用大牲吉，利有攸往。",
      "彖辭": "萃，聚也。順以說，剛中而應，故聚也。王假有廟，致孝享也。利見大人亨，聚以正也。用大牲吉，利有攸往，順天命也。觀其所聚，而天地萬物之情可見矣。",
      "象辭": "澤上於地，萃；君子以除戎器，戒不虞。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "有孚不終，乃亂乃萃，若號，一握為笑，勿恤，往无咎。", "象辭": "乃亂乃萃，其志亂也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "引吉，无咎，孚乃利用禴。", "象辭": "引吉无咎，中未變也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "萃如，嗟如，无攸利，往无咎，小吝。", "象辭": "往无咎，上巽也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "大吉，无咎。", "象辭": "大吉无咎，位不當也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "萃有位，无咎。匪孚，元永貞，悔亡。", "象辭": "萃有位，志未光也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "齎咨涕洟，无咎。", "象辭": "齎咨涕洟，未安上也。" }
      }
    },
    "䷭": {
      "卦名": "升",
      "中文": "地風升",
      "英文": "Pushing Upward",
      "卦序": 46,
      "卦辭": "元亨，用見大人，勿恤，南征吉。",
      "彖辭": "柔以時升，巽而順，剛中而應，是以大亨。用見大人，勿恤﹔有慶也。南征吉，志行也。",
      "象辭": "地中生木，升；君子以順德，積小以高大。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "允升，大吉。", "象辭": "允升大吉，上合志也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "孚乃利用禴，无咎。", "象辭": "九二之孚，有喜也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "升虛邑。", "象辭": "升虛邑，无所疑也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "王用亨于岐山，吉，无咎。", "象辭": "王用亨于岐山，順事也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "貞吉，升階。", "象辭": "貞吉升階，大得志也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "冥升，利于不息之貞。", "象辭": "冥升在上，消不富也。" }
      }
    },
    "䷮": {
      "卦名": "困",
      "中文": "澤水困",
      "英文": "Oppression",
      "卦序": 47,
      "卦辭": "亨，貞，大人吉，无咎，有言不信。",
      "彖辭": "困，剛掩也。險以說，困而不失其所亨，其唯君子乎。貞大人吉，以剛中也。有言不信，尚口乃窮也。",
      "象辭": "澤无水，困﹔君子以致命遂志。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "臀困于株木，入于幽谷，三歲不覿。", "象辭": "入于幽谷，幽不明也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "困于酒食，朱紱方來，利用享祀，征凶，无咎。", "象辭": "困于酒食，中有慶也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "困于石，據于蒺藜，入于其宮，不見其妻，凶。", "象辭": "據于蒺藜，乘剛也。入于其宮，不見其妻，不祥也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "來徐徐，困于金車，吝，有終。", "象辭": "來徐徐，志在下也。雖不當位，有與也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "劓刖，困于赤紱，乃徐有說，利用祭祀。", "象辭": "劓刖，志未得也。乃徐有說，以中直也。利用祭祀，受福也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "困于葛藟，于臲卼，曰動悔。有悔，征吉。", "象辭": "困于葛藟，未當也。動悔有悔，吉行也。" }
      }
    },
    "䷯": {
      "卦名": "井",
      "中文": "水風井",
      "英文": "The Well",
      "卦序": 48,
      "卦辭": "改邑不改井，无喪无得，往來井井。汔至，亦未繘井，羸其瓶，凶。",
      "彖辭": "巽乎水而上水，井；井養而不窮也。改邑不改井，乃以剛中也。汔至亦未繘井，未有功也。羸其瓶，是以凶也。",
      "象辭": "木上有水，井；君子以勞民勸相。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "井泥不食，舊井无禽。", "象辭": "井泥不食，下也。舊井无禽，時舍也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "井谷射鮒，瓮敝漏。", "象辭": "井谷射鮒，无與也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "井渫不食，為我心惻，可用汲，王明，並受其福。", "象辭": "井渫不食，行惻也。求王明，受福也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "井甃，无咎。", "象辭": "井甃无咎，修井也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "井冽，寒泉食。", "象辭": "寒泉之食，中正也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "井收勿幕，有孚元吉。", "象辭": "元吉在上，大成也。" }
      }
    },
    "䷰": {
      "卦名": "革",
      "中文": "澤火革",
      "英文": "Revolution",
      "卦序": 49,
      "卦辭": "己日乃孚，元亨利貞，悔亡。",
      "彖辭": "革，水火相息，二女同居，其志不相得，曰革。己日乃孚﹔革而信之。文明以說，大亨以正，革而當，其悔乃亡。天地革而四時成，湯武革命，順乎天而應乎人，革之時大矣哉。",
      "象辭": "澤中有火，革；君子以治歷明時。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "鞏用黃牛之革。", "象辭": "鞏用黃牛，不可以有為也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "己日乃革之，征吉，无咎。", "象辭": "己日革之，行有嘉也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "征凶，貞厲。革言三就，有孚。", "象辭": "革言三就，又何之矣。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "悔亡，有孚改命，吉。", "象辭": "改命之吉，信志也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "大人虎變，未占有孚。", "象辭": "大人虎變，其文炳也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "君子豹變，小人革面，征凶，居貞吉。", "象辭": "君子豹變，其文蔚也。小人革面，順以從君也。" }
      }
    },
    "䷱": {
      "卦名": "鼎",
      "中文": "火風鼎",
      "英文": "The Cauldron",
      "卦序": 50,
      "卦辭": "元吉，亨。",
      "彖辭": "鼎，象也。以木巽火，亨飪也。聖人亨以享上帝，而大亨以養聖賢。巽而耳目聰明，柔進而上行，得中而應乎剛，是以元亨。",
      "象辭": "木上有火，鼎；君子以正位凝命。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "鼎顛趾，利出否，得妾以其子，无咎。", "象辭": "鼎顛趾，未悖也。利出否，以從貴也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "鼎有實，我仇有疾，不我能即，吉。", "象辭": "鼎有實，慎所之也。我仇有疾，終无尤也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "鼎耳革，其行塞，雉膏不食，方雨虧悔，終吉。", "象辭": "鼎耳革，失其義也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "鼎折足，覆公餗，其形渥，凶。", "象辭": "覆公餗，信如何也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "鼎黃耳金鉉，利貞。", "象辭": "鼎黃耳，中以為實也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "鼎玉鉉，大吉，无不利。", "象辭": "玉鉉在上，剛柔節也。" }
      }
    },
    "䷲": {
      "卦名": "震",
      "中文": "震為雷",
      "英文": "The Arousing Thunder",
      "卦序": 51,
      "卦辭": "亨。震來虩虩，笑言啞啞。震驚百里，不喪匕鬯。",
      "彖辭": "震，亨。震來虩虩，恐致福也。笑言啞啞，後有則也。震驚百里，驚遠而懼邇也。出可以守宗廟社稷，以為祭主也。",
      "象辭": "洊雷，震；君子以恐懼修省。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "震來虩虩，後笑言啞啞，吉。", "象辭": "震來虩虩，恐致福也。笑言啞啞，後有則也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "震來厲，億喪貝，躋于九陵，勿逐，七日得。", "象辭": "震來厲，乘剛也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "震蘇蘇，震行无眚。", "象辭": "震蘇蘇，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "震遂泥。", "象辭": "震遂泥，未光也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "震往來厲，億无喪，有事。", "象辭": "震往來厲，危行也。其事在中，大无喪也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "震索索，視矍矍，征凶。震不于其躬，于其鄰，无咎。婚媾有言。", "象辭": "震索索，中未得也。雖凶无咎，畏鄰戒也。" }
      }
    },
    "䷳": {
      "卦名": "艮",
      "中文": "艮為山",
      "英文": "Keeping Still Mountain",
      "卦序": 52,
      "卦辭": "艮其背，不獲其身，行其庭，不見其人，无咎。",
      "彖辭": "艮，止也。時止則止，時行則行，動靜不失其時，其道光明。艮其止，止其所也。上下敵應，不相與也。是以不獲其身，行其庭不見其人，无咎也。",
      "象辭": "兼山，艮；君子以思不出其位。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "艮其趾，无咎，利永貞。", "象辭": "艮其趾，未失正也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "艮其腓，不拯其隨，其心不快。", "象辭": "不拯其隨，未退聽也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "艮其限，列其夤，厲薰心。", "象辭": "艮其限，危薰心也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "艮其身，无咎。", "象辭": "艮其身，止諸躬也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "艮其輔，言有序，悔亡。", "象辭": "艮其輔，以中正也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "敦艮，吉。", "象辭": "敦艮之吉，以厚終也。" }
      }
    },
    "䷴": {
      "卦名": "漸",
      "中文": "風山漸",
      "英文": "Development",
      "卦序": 53,
      "卦辭": "女歸吉，利貞。",
      "彖辭": "漸之進也，女歸吉也。進得位，往有功也。進以正，可以正邦也。其位剛，得中也。止而巽，動不窮也。",
      "象辭": "山上有木，漸；君子以居賢德善俗。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "鴻漸于干，小子厲，有言，无咎。", "象辭": "小子厲，義无咎也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "鴻漸于磐，飲食衎衎，吉。", "象辭": "飲食衎衎，不素飽也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "鴻漸于陸，夫征不復，婦孕不育，凶；利禦寇。", "象辭": "夫征不復，離群丑也。婦孕不育，失其道也。利用禦寇，順相保也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "鴻漸于木，或得其桷，无咎。", "象辭": "或得其桷，順以巽也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "鴻漸于陵，婦三歲不孕，終莫之勝，吉。", "象辭": "終莫之勝，吉﹔得所愿也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "鴻漸于陸，其羽可用為儀，吉。", "象辭": "其羽可用為儀，吉﹔不可亂也。" }
      }
    },
    "䷵": {
      "卦名": "歸妹",
      "中文": "雷澤歸妹",
      "英文": "The Marrying Maiden",
      "卦序": 54,
      "卦辭": "征凶，无攸利。",
      "彖辭": "歸妹，天地之大義也。天地不交，而萬物不興。歸妹，人之終始也。說以動，所歸妹也。征凶，位不當也。无攸利，柔乘剛也。",
      "象辭": "澤上有雷，歸妹；君子以永終知敝。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "歸妹以娣，跛能履，征吉。", "象辭": "歸妹以娣，以恆也。跛能履，吉，相承也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "眇能視，利幽人之貞。", "象辭": "利幽人之貞，未變常也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "歸妹以須，反歸以娣。", "象辭": "歸妹以須，未當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "歸妹愆期，遲歸有時。", "象辭": "愆期之志，有待而行也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "帝乙歸妹，其君之袂，不如其娣之袂良，月幾望，吉。", "象辭": "帝乙歸妹，不如其娣之袂良也。其位在中，以貴行也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "女承筐无實，士刲羊无血，无攸利。", "象辭": "上六无實，承虛筐也。" }
      }
    },
    "䷶": {
      "卦名": "豐",
      "中文": "雷火豐",
      "英文": "Abundance",
      "卦序": 55,
      "卦辭": "亨，王假之，勿憂，宜日中。",
      "彖辭": "豐，大也。明以動，故豐。王假之，尚大也。勿憂宜日中，宜照天下也。日中則昃，月盈則食，天地盈虛，與時消息，而況於人乎，況於鬼神乎。",
      "象辭": "雷電皆至，豐；君子以折獄致刑。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "遇其配主，雖旬无咎，往有尚。", "象辭": "雖旬无咎，過旬災也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "豐其蔀，日中見斗，往得疑疾，有孚發若，吉。", "象辭": "有孚發若，信以發志也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "豐其沛，日中見沫，折其右肱，无咎。", "象辭": "豐其沛，不可大事也。折其右肱，終不可用也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "豐其蔀，日中見斗，遇其夷主，吉。", "象辭": "豐其蔀，位不當也。日中見斗，幽不明也。遇其夷主，吉﹔行也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "來章，有慶譽，吉。", "象辭": "六五之吉，有慶也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "豐其屋，蔀其家，窺其戶，闃其无人，三歲不見，凶。", "象辭": "豐其屋，天際翔也。窺其戶，闃其无人，自藏也。" }
      }
    },
    "䷷": {
      "卦名": "旅",
      "中文": "火山旅",
      "英文": "The Wanderer",
      "卦序": 56,
      "卦辭": "小亨，旅貞吉。",
      "彖辭": "旅，小亨，柔得中乎外，而順乎剛，止而麗乎明，是以小亨，旅貞吉也。旅之時義大矣哉。",
      "象辭": "山上有火，旅；君子以明慎用刑，而不留獄。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "旅瑣瑣，斯其所取災。", "象辭": "旅瑣瑣，志窮災也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "旅即次，懷其資，得童僕貞。", "象辭": "得童僕貞，終无尤也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "旅焚其次，喪其童僕，貞厲。", "象辭": "旅焚其次，亦以傷矣。以旅與下，其義喪也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "旅于處，得其資斧，我心不快。", "象辭": "旅于處，未得位也。得其資斧，心未快也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "射雉，一矢亡，終以譽命。", "象辭": "終以譽命，上逮也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "鳥焚其巢，旅人先笑後號咷。喪牛于易，凶。", "象辭": "以旅在上，其義焚也。喪牛于易，終莫之聞也。" }
      }
    },
    "䷸": {
      "卦名": "巽",
      "中文": "巽為風",
      "英文": "The Gentle Wind",
      "卦序": 57,
      "卦辭": "小亨，利有攸往，利見大人。",
      "彖辭": "重巽以申命。剛上而柔下，巽而順，是以小亨，利有攸往，利見大人。",
      "象辭": "隨風，巽；君子以申命行事。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "進退，利武人之貞。", "象辭": "進退，志疑也。利武人之貞，志治也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "巽在床下，用史巫紛若，吉，无咎。", "象辭": "紛若之吉，得中也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "頻巽，吝。", "象辭": "頻巽之吝，志窮也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "悔亡，田獲三品。", "象辭": "田獲三品，有功也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "貞吉，悔亡，无不利。无初有終，先庚三日，後庚三日，吉。", "象辭": "九五之吉，位正中也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "巽在床下，喪其資斧，貞凶。", "象辭": "巽在床下，上窮也。喪其資斧，正乎凶也。" }
      }
    },
    "䷹": {
      "卦名": "兌",
      "中文": "兌為澤",
      "英文": "The Joyous Lake",
      "卦序": 58,
      "卦辭": "亨，利貞。",
      "彖辭": "兌，說也。剛中而柔外，說以利貞，是以順乎天，而應乎人。說以先民，民忘其勞；說以犯難，民忘其死；說之大，民勸矣哉！",
      "象辭": "麗澤，兌；君子以朋友講習。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "和兌，吉。", "象辭": "和兌之吉，行未疑也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "孚兌，吉，悔亡。", "象辭": "孚兌之吉，信志也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "來兌，凶。", "象辭": "來兌之凶，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "商兌，未寧，介疾有喜。", "象辭": "九四之喜，有慶也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "孚于剝，有厲。", "象辭": "孚于剝，位正當也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "引兌。", "象辭": "上六引兌，未光也。" }
      }
    },
    "䷺": {
      "卦名": "渙",
      "中文": "風水渙",
      "英文": "Dispersion",
      "卦序": 59,
      "卦辭": "亨，王假有廟，利涉大川，利貞。",
      "彖辭": "渙，亨。剛來而不窮，柔得位乎外而上同。王假有廟，王乃在中也。利涉大川，乘木有功也。",
      "象辭": "風行水上，渙；先王以享于帝立廟。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "用拯馬壯，吉。", "象辭": "初六之吉，順也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "渙奔其機，悔亡。", "象辭": "渙奔其機，得願也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "渙其躬，无悔。", "象辭": "渙其躬，志在外也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "渙其群，元吉。渙有丘，匪夷所思。", "象辭": "渙其群，元吉﹔光大也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "渙汗其大號，渙王居，无咎。", "象辭": "王居无咎，正位也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "渙其血，去逖出，无咎。", "象辭": "渙其血，遠害也。" }
      }
    },
    "䷻": {
      "卦名": "節",
      "中文": "水澤節",
      "英文": "Limitation",
      "卦序": 60,
      "卦辭": "亨。苦節不可貞。",
      "彖辭": "節，亨，剛柔分，而剛得中。苦節不可貞，其道窮也。說以行險，當位以節，中正以通。天地節而四時成，節以制度，不傷財，不害民。",
      "象辭": "澤上有水，節；君子以制數度，議德行。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "不出戶庭，无咎。", "象辭": "不出戶庭，知通塞也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "不出門庭，凶。", "象辭": "不出門庭，失時極也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "不節若，則嗟若，无咎。", "象辭": "不節之嗟，又誰咎也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "安節，亨。", "象辭": "安節之亨，承上道也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "甘節，吉，往有尚。", "象辭": "甘節之吉，居位中也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "苦節，貞凶，悔亡。", "象辭": "苦節貞凶，其道窮也。" }
      }
    },
    "䷼": {
      "卦名": "中孚",
      "中文": "風澤中孚",
      "英文": "Inner Truth",
      "卦序": 61,
      "卦辭": "豚魚吉，利涉大川，利貞。",
      "彖辭": "中孚，柔在內而剛得中。說而巽，孚，乃化邦也。豚魚吉，信及豚魚也。利涉大川，乘木舟虛也。中孚以利貞，乃應乎天也。",
      "象辭": "澤上有風，中孚；君子以議獄緩死。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "虞吉，有它不燕。", "象辭": "初九虞吉，志未變也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "鳴鶴在陰，其子和之，我有好爵，吾與爾靡之。", "象辭": "其子和之，中心愿也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "得敵，或鼓或罷，或泣或歌。", "象辭": "或鼓或罷，位不當也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "月幾望，馬匹亡，无咎。", "象辭": "馬匹亡，絕類上也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "有孚攣如，无咎。", "象辭": "有孚攣如，位正當也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "翰音登于天，貞凶。", "象辭": "翰音登于天，何可長也。" }
      }
    },
    "䷽": {
      "卦名": "小過",
      "中文": "雷山小過",
      "英文": "Small Exceeding",
      "卦序": 62,
      "卦辭": "亨，利貞，可小事，不可大事。飛鳥遺之音，不宜上，宜下，大吉。",
      "彖辭": "小過，小者過而亨也。過以利貞，與時行也。柔得中，是以小事吉也。剛失位而不中，是以不可大事也。有飛鳥之象焉，飛鳥遺之音，不宜上，宜下，大吉﹔上逆而下順也。",
      "象辭": "山上有雷，小過﹔君子以行過乎恭，喪過乎哀，用過乎儉。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "飛鳥以凶。", "象辭": "飛鳥以凶，不可如何也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "過其祖，遇其妣﹔不及其君，遇其臣﹔无咎。", "象辭": "不及其君，臣不可過也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "弗過防之，從或戕之，凶。", "象辭": "從或戕之，凶如何也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "无咎，弗過遇之。往厲必戒，勿用永貞。", "象辭": "弗過遇之，位不當也。往厲必戒，終不可長也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "密雲不雨，自我西郊，公弋取彼在穴。", "象辭": "密雲不雨，已上也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "弗遇過之，飛鳥離之，凶，是謂災眚。", "象辭": "弗遇過之，已亢也。" }
      }
    },
    "䷾": {
      "卦名": "既濟",
      "中文": "水火既濟",
      "英文": "After Completion",
      "卦序": 63,
      "卦辭": "亨小，利貞，初吉終亂。",
      "彖辭": "既濟，亨，小者亨也。利貞，剛柔正而位當也。初吉，柔得中也。終止則亂，其道窮也。",
      "象辭": "水在火上，既濟；君子以思患而預防之。",
      "爻辭": {
        "初": { "陰陽":"初九", "數":9, "爻辭": "曳其輪，濡其尾，无咎。", "象辭": "曳其輪，義无咎也。" },
        "二": { "陰陽":"六二", "數":6, "爻辭": "婦喪其茀，勿逐，七日得。", "象辭": "七日得，以中道也。" },
        "三": { "陰陽":"九三", "數":9, "爻辭": "高宗伐鬼方，三年克之，小人勿用。", "象辭": "三年克之，憊也。" },
        "四": { "陰陽":"六四", "數":6, "爻辭": "繻有衣袽，終日戒。", "象辭": "終日戒，有所疑也。" },
        "五": { "陰陽":"九五", "數":9, "爻辭": "東鄰殺牛，不如西鄰之禴祭，實受其福。", "象辭": "東鄰殺牛，不如西鄰之時也。實受其福，吉大來也。" },
        "上": { "陰陽":"上六", "數":6, "爻辭": "濡其首，厲。", "象辭": "濡其首厲，何可久也。" }
      }
    },
    "䷿": {
      "卦名": "未濟",
      "中文": "火水未濟",
      "英文": "Unfinished",
      "卦序": 64,
      "卦辭": "亨，小狐汔濟，濡其尾，无攸利。",
      "彖辭": "未濟，亨，柔得中也。小狐汔濟，未出中也。濡其尾，无攸利，不續終也。雖不當位，剛柔應也。",
      "象辭": "火在水上，未濟；君子以慎辨物居方。",
      "爻辭": {
        "初": { "陰陽":"初六", "數":6, "爻辭": "濡其尾，吝。", "象辭": "濡其尾，亦不知極也。" },
        "二": { "陰陽":"九二", "數":9, "爻辭": "曳其輪，貞吉。", "象辭": "九二貞吉，中以行正也。" },
        "三": { "陰陽":"六三", "數":6, "爻辭": "未濟，征凶，利涉大川。", "象辭": "未濟征凶，位不當也。" },
        "四": { "陰陽":"九四", "數":9, "爻辭": "貞吉，悔亡，震用伐鬼方，三年有賞于大國。", "象辭": "貞吉悔亡，志行也。" },
        "五": { "陰陽":"六五", "數":6, "爻辭": "貞吉，无悔，君子之光，有孚，吉。", "象辭": "君子之光，其暉吉也。" },
        "上": { "陰陽":"上九", "數":9, "爻辭": "有孚于飲酒，无咎，濡其首，有孚失是。", "象辭": "飲酒濡首，亦不知節也。" }
      }
    }
  }
}
